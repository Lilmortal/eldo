{
  "name": "@eldo/app",
  "version": "2.2.5",
  "description": "Main Eldo application",
  "main": "./webpack.dev.js",
  "repository": "https://github.com/Lilmortal/eldo.git",
  "author": "Jack Tan <jacktan165@gmail.com>",
  "keywords": [
    "eldo"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "webpack-dev-server  --open --config ./webpack.dev.js",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint:css": "stylelint **/*.css --fix --allow-empty-input",
    "lint:ts": "eslint **/*.ts{,x} --fix --no-error-on-unmatched-pattern",
    "lint": "run-p -c 'lint:* --fix'",
    "build": "webpack --config ./webpack.prod.js"
  },
  "devDependencies": {
    "@eldo/eslint-config-lint-tool": "^2.3.0",
    "@eldo/jest-config": "^2.0.13",
    "@eldo/webpack": "^2.3.0",
    "@storybook/react": "^5.3.12",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@eldo/button": "^2.5.0",
    "@eldo/styles": "^1.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
